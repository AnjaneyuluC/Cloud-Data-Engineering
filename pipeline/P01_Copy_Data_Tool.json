{
	"name": "P01_Copy_Data_Tool",
	"properties": {
		"activities": [
			{
				"name": "Copy_ii4",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "adfv2tutorial//inputEmp.TXT"
					},
					{
						"name": "Destination",
						"value": "dbo.inputEmp"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "Src_Input_EMP",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "tgt_emp",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}